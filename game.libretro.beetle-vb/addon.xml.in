<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-vb"
		name="Nintendo - Virtual Boy (Beetle VB)"
		version="1.27.1.18"
		provider-name="Mednafen Team">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>GB</platforms>
		<extensions>vb|vboy|bin</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.beetle-vb</source>
		<assets>
			<icon>resources/icon.png</icon>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<summary lang="en_GB">Nintendo - Virtual Boy (Beetle VB) v1.27.1</summary>
		<description lang="en_GB">Beetle VB emulates the Nintendo Virtual Boy, a tabletop video game console capable of displaying stereoscopic &#34;3D&#34; graphics.</description>
	</extension>
</addon>
